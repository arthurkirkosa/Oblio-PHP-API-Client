{
  "name": "arthurkirkosa/oblio-php-client",
  "description": "Oblio PHP API Client",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Zoltan Szanto",
      "email": "mrbig00@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "": "src/"
    }
  },
  "require": {
    "php": "^7.2.0|^8.0",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^7.0",
    "kamermans/guzzle-oauth2-subscriber": "^1.0",
    "guzzlehttp/guzzle-services": "^1.2",
    "arthurkirkosa/guzzle-description-loader": "^0.0.7"
  },
  "config": {
      "optimize-autoloader": true,
      "preferred-install": "dist",
      "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
